{
    "cSpell.enableFiletypes": ["python"],
    "cSpell.words": [
        "Bosonic",
        "cython",
        "dtype",
        "montecarlo",
        "ndarray",
        "randomgenerator",
        "thermalization",
        "thermalize",
        "thermalized"
    ],
    "files.exclude": {
        "**/*.fdb_latexmk": true,
        "**/*.fls": true,
        "**/*.log": true,
        "**/*.aux": true,
        "**/*.bcf": true,
        "**/*.out": true,
        "**/*.run.xml": true,
        "**/*.synctex.gz": true,
        "**/*.synctex(busy)": true,
        "**/*.toc": true,
        "**/*.bbl": true,
        "**/*.blg": true,
        "**/*-blx.bib": true,
        "**/_minted*": true,
        "**/__pycache__": true
    }
}
